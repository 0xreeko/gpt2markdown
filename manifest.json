{
    "manifest_version": 1,
    "name": "GPTExpo",
    "version": "1.0.0",
    "description": "Export your conversations with ChatGPT to Notion.",
    "permissions": [
        "activeTab",
        "https://*.notion.com/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://chat.openai.com/*"
            ],
            "js": [
                "script.js"
            ]
        }
    ]
}