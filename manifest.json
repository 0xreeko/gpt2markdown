{
    "manifest_version": 3,
    "name": "GPTExpo",
    "version": "1.0.0",
    "description": "Export your conversations with ChatGPT to Notion.",
    "icons": {
        "16": "images/favicon-16x16.png"
    },
    "page_action": {
        "default_action": {
            "16": "images/favicon-16x16.png"
        }
    },
    "permissions": [
        "activeTab",
        "https://*.notion.com/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://chat.openai.com/*"
            ],
            "js": [
                "script.js"
            ]
        }
    ]
}